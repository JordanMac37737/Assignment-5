package Assignment5;

import java.time.LocalDate;
	import java.time.format.DateTimeFormatter;
	
public class Person {

	String name;
	String lastname;
	int age;
	LocalDate birthdate;

public Person(String name, String lastname, int age, LocalDate birthdate) {
	this.name = name;
	this.lastname = lastname;
	this.age = age;
	this.birthdate = birthdate;
}	
		
public void printInfo() {
//Insert final text that shows name, last name, age, birthdate, and a final message saying thank you
	System.out.println("Client: " + name +" "+ lastname);
	System.out.println("Age: " + age);
	System.out.println("DOB: " + birthdate);
	System.out.println("Thank you " + name + " for registering with us!");
}
	}


